		"dev": "npm run patches; NODE_ENV=development node server.js",
		"beta": "NODE_ENV=beta node server.js",
		"build-production": "NODE_ENV=production next build",
		"build-beta": "NODE_ENV=beta next build",
		"build-dev": "NODE_ENV=development next build",
		"start-production": "NODE_ENV=production node server.js",
		"export": "next export",
		"test": "jest --detectOpenHandles --testTimeout=60000",
		"test-all": "node tests/test-all.js",
		"pull-production": "git pull origin production-build; git reset --hard origin/production-build && npm install && npm run patches && pm2 startOrGracefulReload pm2-production.config.js && sleep 15 && sudo rm -rf /data/nginx/cache/*",
		"pull-beta": "git pull origin beta-build; git reset --hard origin/beta-build && npm install && npm run patches && pm2 startOrGracefulReload pm2-beta.config.js"
	},
